<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.meishe.myvideo.view.MYTimelineEditorRecyclerView android:id="@+id/editor_timeline_view_time_making_line_recycler" android:layout_width="fill_parent" android:layout_height="20.0dip" android:layout_marginTop="5.0dip" />
    <RelativeLayout android:id="@+id/editor_main_track_parent" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/multi_thumbnail_sequence_main_margin">
        <com.meishe.myvideo.ui.trackview.MultiThumbnailSequenceView android:id="@+id/editor_multi_thumbnail_sequence_view" android:scrollbars="none" android:layout_width="fill_parent" android:layout_height="@dimen/multi_thumbnail_sequence_height" android:overScrollMode="never" />
        <com.meishe.myvideo.view.MYTimelineEditorRecyclerView android:id="@+id/editor_timeline_view_trans_recycler" android:layout_width="fill_parent" android:layout_height="@dimen/multi_thumbnail_sequence_height" />
