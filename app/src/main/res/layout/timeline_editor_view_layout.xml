            <LinearLayout android:orientation="vertical" android:id="@+id/ll_close_voice" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true">
                <Button android:layout_gravity="center_horizontal" android:id="@+id/btn_close_original_voice" android:background="@drawable/cb_close_voice" android:layout_width="@dimen/menu_item_image_width" android:layout_height="@dimen/menu_item_image_height" />
                <TextView android:textSize="@dimen/close_original_voice_text_size" android:textColor="@color/white_8" android:id="@+id/tv_close_voice_desc" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp14" android:text="@string/close_original_voice" />
            </LinearLayout>
        </RelativeLayout>
        <ImageView android:id="@+id/editor_add_clip_img" android:background="@mipmap/ic_add_material" android:layout_width="25.0dip" android:layout_height="25.0dip" android:layout_marginTop="12.0dip" android:layout_marginRight="@dimen/dp32" android:layout_alignParentRight="true" />
    </RelativeLayout>
    <com.meishe.myvideo.view.MYLineView android:id="@+id/editor_line_view" android:background="@color/timeline_bg_color" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_alignTop="@id/editor_main_track_parent" />
</RelativeLayout>